{
  "name": "todoui",
  "version": "0.0.1",
  "description": "",
  "productName": "Todo App",
  "author": "Deven Phillips <deven.phillips@redhat.com>",
  "private": true,
  "watch": {
    "build:client": "openapi.yml"
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "build:client": "openapi-generator generate -i openapi.yml -g typescript-axios -o src/apiClient",
    "watch": "npm-watch",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.1",
    "core-js": "^3.6.5",
    "moment": "^2.27.0",
    "quasar": "^1.0.0",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.3.4"
  },
  "devDependencies": {
    "@jest/globals": "^26.1.0",
    "@openapitools/openapi-generator-cli": "^1.0.15-4.3.1",
    "@quasar/app": "^2.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.1",
    "@types/jest": "^26.0.7",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.17.15",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^26.1.0",
    "npm-watch": "^0.6.0"
  },
  "browserslist": [
    "ie >= 11",
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
